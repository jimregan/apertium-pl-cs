  <mode name="pl-cs" install="yes">
    <pipeline>
      <program name="lt-proc">
        <file name="pl-cs.automorf.bin"/>
      </program>
      <program name="apertium-tagger -g $2">
        <file name="pl-cs.prob"/>
      </program>
      <program name="apertium-pretransfer"/>
      <program name="apertium-transfer">
        <file name="apertium-pl-cs.pl-cs.t1x"/>
