        <file name="pl-cs.autobil.bin"/>
      </program>
      <program name="apertium-interchunk">
        <file name="apertium-pl-cs.pl-cs.t2x"/>
        <file name="pl-cs.t2x.bin"/>
      </program>
      <program name="apertium-postchunk">
        <file name="apertium-pl-cs.pl-cs.t3x"/>
        <file name="pl-cs.t3x.bin"/>
      </program>
      <program name="lt-proc $1">
        <file name="pl-cs.autogen.bin"/>
      </program>
      <program name="lt-proc -p">
        <file name="pl-cs.autopgen.bin"/>
      </program>
    </pipeline>
  </mode>

